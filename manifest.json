{
  "manifest_version": 2,

  "name": "Gui",
  "description": "This extension enables to store multiple tabs",
  "version": "1.0",
  
  "background" : { 
	"scripts": ["background.js"] , "persistent": true
  },
  
  "browser_action": {
      "default_icon": "icon.png",
      "default_title": "Running TAB++"
   },
   
    "permissions": [
      "bookmarks",
      "http://*.google.com/bookmarks/*",
      "tabs"
    ]

}
